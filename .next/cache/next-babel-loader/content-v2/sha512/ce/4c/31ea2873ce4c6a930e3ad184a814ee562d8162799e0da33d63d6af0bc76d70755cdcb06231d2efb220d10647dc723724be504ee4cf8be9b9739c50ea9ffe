;\r\n  const [loading, setLoading] = useState(false);\r\n  const [perPage, setPerPage] = useState(10);\r\n  const [currentPage, setCurrentPage] = useState(1);\r\n  const [totalRows, setTotalRows] = useState(0);\r\n  const [id, setId] = useState(null);\r\n  const cookies = new Cookies();\r\n\r\n  useEffect(() => {\r\n    getCandidate();\r\n  }, []);\r\n\r\n  const router = useRouter();\r\n  const eventId = router.query.eventId;\r\n\r\n  const getCandidate = async () => {\r\n    setLoading(true);\r\n    const { data: res } = await axios.get(\r\n      `${process.env.NEXT_PUBLIC_URL}/candidate/${eventId}/?page=${currentPage}&size=${perPage}`\r\n    );\r\n    setTotalRows(res.data.totalData);\r\n    setCandidateDetail(res.data.content);\r\n    setLoading(false);\r\n  };\r\n\r\n  const columnCalon = [\r\n    {\r\n      name: \"Nama\",\r\n      selector: (row) => row.candidate_name,\r\n    },\r\n    {\r\n      name: \"Foto\",\r\n      cell: (row) => (\r\n        <img\r\n          src={row.photo_url}\r\n          alt={row.photo_url}\r\n          style={{ width: \"50px\" }}\r\n        />\r\n      ),\r\n    },\r\n    {\r\n      name: \"Action\",\r\n      cell: (row) => (\r\n        <>\r\n          <Button\r\n            onClick={() => handleEdit(row)}\r\n            outline\r\n            color=\"primary\"\r\n            size=\"sm\"\r\n          >\r\n            Edit\r\n          </Button>\r\n          <Button\r\n            onClick={() => handleDelete(row.id)}\r\n            outline\r\n            color=\"danger\"\r\n            size=\"sm\"\r\n          >\r\n            Hapus\r\n          </Button>\r\n        </>\r\n      ),\r\n    },\r\n  ];\r\n\r\n  const handleEdit = (row) => {\r\n    setSelectedData(row);\r\n    toggleModalCalon(false);\r\n  };\r\n\r\n  const handleDelete = (id) => {\r\n    Swal.fire({\r\n      title: \"Anda Yakin ?\",\r\n      text: \"Data yang telah dihapus tidak dapat dikembalikan!\",\r\n      icon: \"warning\",\r\n      showCancelButton: true,\r\n      confirmButtonColor: \"#3085d6\",\r\n      cancelButtonColor: \"#d33\",\r\n      confirmButtonText: \"Yakin!\",\r\n    }).then((result) => {\r\n      if (result.isConfirmed) {\r\n        axios\r\n          .delete(`${process.env.NEXT_PUBLIC_URL}/candidate/${id} `, {\r\n            headers: {\r\n              Authorization: `Bearer ${cookies.get(\"token\")}`,\r\n            },\r\n          })\r\n          .then((res) => {\r\n            Swal.fire({\r\n              icon: \"success\",\r\n              title: \"Berhasil\",\r\n              text: \"Data Berhasil Dihapus !\",\r\n            });\r\n            getCandidate();\r\n          });\r\n      }\r\n    });\r\n  };\r\n\r\n  const toggleModalCalon = (tambahMode) => {\r\n    setModalCalon(!modalCalon);\r\n  };\r\n\r\n  const handleSubmitEvent = async (id) => {\r\n    const formData = new FormData();\r\n    formData.append(`${eventId}`, id);\r\n\r\n    // let data = {\r\n    //   event_id: `${eventId}`,\r\n    //   candidate_name: namaKandidat,\r\n    //   photo_url: fotoKandidat,\r\n    // };\r\n    console.log(data);\r\n    if (action === \"create\") {\r\n      await axios \r\n        .post(`${process.env.NEXT_PUBLIC_URL}/candidate/${eventId}`, data, {\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n            Authorization: `Bearer ${cookies.get(\"token\")}`,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          Swal.fire({\r\n            icon: \"success\",\r\n            title: \"Berhasil\",\r\n            text: \"Data Berhasil Disimpan !\",\r\n          });\r\n          setModal(false);\r\n          getCandidate();\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n          if (e.response.data.error_messages) {\r\n            Swal.fire({\r\n              icon: \"error\",\r\n              title: \"Terjadi Kesalahan!\",\r\n              text: e.response.data.error_messages[0].msg,\r\n            });\r\n          } else {\r\n            Swal.fire({\r\n              icon: \"error\",\r\n              title: \"Terjadi Kesalahan!\",\r\n              text: e,\r\n            });\r\n          }\r\n        });\r\n    }\r\n  };\r\n\r\n  let onClosedModal = () => {\r\n    setId(null);\r\n    setNamaKandidat(\"\");\r\n    setfotoKandidat(\"\");\r\n  };\r\n\r\n  let handleChange = (e, field) => {\r\n    let data = e.target.value;\r\n    if (field === \"kandidat\") {\r\n      setNamaKandidat(data);\r\n    } else if (field === \"foto\") {\r\n      console.log(e)\r\n      setfotoKandidat(data);\r\n    } else {\r\n      Swal.fire({\r\n        icon: \"error\",\r\n        title: \"Oops...\",\r\n        text: \"Something went wrong!\",\r\n      });\r\n    }\r\n  };\r\n\r\n  const handlePageChange = (page) => {\r\n    setCurrentPage(page);\r\n  };\r\n\r\n  const handlePerRowsChange = async (newPerPage, page) => {\r\n    setPerPage(newPerPage);\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <Card className=\"mt-2 mb-2\" color=\"secondary\">\r\n        <CardBody>\r\n          <Button color=\"primary\" onClick={() => toggleModalCalon(true)}>\r\n            Tambah\r\n          </Button>\r\n        </CardBody>\r\n      </Card>\r\n\r\n      {/* table data calon */}\r\n      <DataTable \r\n        columns={columnCalon} \r\n        data={dataCandidate} \r\n        pagination\r\n        progressPending={loading}\r\n        paginationTotalRows={totalRows}\r\n        paginationDefaultPage={currentPage}\r\n        onChangeRowsPerPage={handlePerRowsChange}\r\n        onChangePage={handlePageChange}\r\n        />\r\n\r\n      {/* modal edit calon */}\r\n      <Modal isOpen={modalCalon} toggle={toggleModalCalon} onClosed={onClosedModal}>\r\n        <ModalHeader toggle={toggleModalCalon}>Modal title</ModalHeader>\r\n        <ModalBody>\r\n          <Form>\r\n            <FormGroup>\r\n              <Label for=\"namaKandidat\">Event</Label>\r\n              <Input\r\n                id=\"namaKandidat\"\r\n                name=\"nama\"\r\n                placeholder=\"Nama Calon\"\r\n                type=\"text\"\r\n                value={namaKandidat}\r\n                onChange={(e) => handleChange(e, \"kandidat\")}\r\n              />\r\n            </FormGroup>\r\n            <FormGroup>\r\n              <Label for=\"fotoKandidat\">Foto Calon</Label>\r\n              <Input\r\n                id=\"fotoKandidat\"\r\n                name=\"foto\"\r\n                type=\"file\"\r\n                value={fotoKandidat}\r\n                onChange={(e) => handleChange(e, \"foto\")}\r\n              />\r\n            </FormGroup>\r\n          </Form>\r\n        </ModalBody>\r\n        <ModalFooter>\r\n          <Button color=\"primary\" onClick={() => handleSubmitEvent()}>\r\n            Simpan\r\n          </Button>\r\n        </ModalFooter>\r\n      </Modal>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default TableCalon;\r\n"]},"metadata":{},"sourceType":"module"}