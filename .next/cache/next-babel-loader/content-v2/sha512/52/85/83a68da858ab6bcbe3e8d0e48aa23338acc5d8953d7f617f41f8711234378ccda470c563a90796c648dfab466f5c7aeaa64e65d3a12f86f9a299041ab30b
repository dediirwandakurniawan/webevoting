Kandidat] = useState(\"\");\r\n  const [hideFotoEvent, setHideFotoEvent] = useState(false);\r\n  const [action, setAction] = useState(\"create\");\r\n  const [events, setEvents] = useState([]);\r\n  const [id, setId] = useState(null);\r\n  const cookies = new Cookies();\r\n\r\n  useEffect(() => {\r\n    const getEvent = async () => {\r\n      const { data: res } = await axios.get(`http://192.168.1.35:3030/event`);\r\n      setEvents(res.data.content);\r\n    };\r\n    getEvent();\r\n  }, []);\r\n\r\n  const toggle = () => setModal(!modal);\r\n  const columns = [\r\n    {\r\n      name: \"Nama Event\",\r\n      selector: (row) => (row.event_name ? row.event_name : \"-\"),\r\n    },\r\n    {\r\n      name: \"Deskripsi\",\r\n      selector: (row) => (row.description ? row.description : \"-\"),\r\n    },\r\n    {\r\n      name: \"Active\",\r\n      selector: (row) => {\r\n        {\r\n          {\r\n            row.is_active === true ? (\r\n              <Badge color=\"primary\" pill>\r\n                Yes\r\n              </Badge>\r\n            ) : (\r\n              <Badge color=\"danger\" pill>\r\n                No\r\n              </Badge>\r\n            );\r\n          }\r\n        }\r\n      },\r\n    },\r\n    {\r\n      name: \"Kuota Vote\",\r\n      selector: (row) => row.quota,\r\n    },\r\n    {\r\n      name: \"Sembunyikan Foto\",\r\n      selector: (row) => {\r\n        {\r\n          {\r\n            row.show_photo === true ? (\r\n              <Badge color=\"primary\" pill>\r\n                Yes\r\n              </Badge>\r\n            ) : (\r\n              <Badge color=\"danger\" pill>\r\n                No\r\n              </Badge>\r\n            );\r\n          }\r\n        }\r\n      },\r\n    },\r\n    {\r\n      name: \"Action\",\r\n      selector: (row) => (\r\n        <>\r\n          <a onClick={() => handleEditForm()}>Edit</a> | <a>Delete</a> |{\" \"}\r\n          <a href=\"/admin/event/eventDetail\">Detail</a>\r\n        </>\r\n      ),\r\n