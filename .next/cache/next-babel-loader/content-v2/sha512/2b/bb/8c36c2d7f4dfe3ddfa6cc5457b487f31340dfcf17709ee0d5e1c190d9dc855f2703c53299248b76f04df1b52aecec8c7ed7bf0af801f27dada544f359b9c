IdEventActive(true);\r\n  };\r\n\r\n  let handleSubmitToken = async () => {\r\n    const data = {\r\n      token,\r\n    };\r\n    await axios\r\n      .post(`${process.env.NEXT_PUBLIC_URL}/token/check-token/${idEvent}`, data, {\r\n        headers: {\r\n          Authorization: `Bearer ${cookies.get(\"token\")}`,\r\n        },\r\n      })\r\n      .then((res) => {\r\n        setTokenActive(true);\r\n        setDataKandidat(res.data.data.content);\r\n      })\r\n      .catch((e) => {\r\n        console.log(e);\r\n        if (e.response.data.message) {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Terjadi Kesalahan!\",\r\n            text: e.response.data.message,\r\n          });\r\n        } else {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Terjadi Kesalahan!\",\r\n            text: e,\r\n          });\r\n        }\r\n      });\r\n  };\r\n\r\n  let handleVote = async () => {\r\n    const data = {\r\n      token,\r\n      candidate_id: kandidatDipilih,\r\n    };\r\n\r\n    await axios\r\n      .post(`${process.env.NEXT_PUBLIC_URL}/vote/${idEvent}`, data, {\r\n        headers: {\r\n          Authorization: `Bearer ${cookies.get(\"token\")}`,\r\n        },\r\n      })\r\n      .then((res) => {\r\n        Swal.fire({\r\n          icon: \"success\",\r\n          title: \"Berhasil\",\r\n          text: \"Vote Berhasil\",\r\n          timer: 2000,\r\n        });\r\n        setTokenActive(false);\r\n        setKandidatDipilih([]);\r\n        setToken(\"\");\r\n      })\r\n      .catch((e) => {\r\n        console.log(e);\r\n        if (e?.response?.data?.message) {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Terjadi Kesalahan!\",\r\n            text: e.response.data.message,\r\n          });\r\n        } else {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Terjadi Kesalahan!\",\r\n            text: e,\r\n          });\r\n        }\r