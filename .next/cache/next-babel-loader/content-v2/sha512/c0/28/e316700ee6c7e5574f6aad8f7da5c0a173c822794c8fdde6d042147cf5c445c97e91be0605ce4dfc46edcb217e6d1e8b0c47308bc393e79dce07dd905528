       console.log(e);\r\n          if (e.response.data.error_messages) {\r\n            Swal.fire({\r\n              icon: \"error\",\r\n              title: \"Terjadi Kesalahan!\",\r\n              text: e.response.data.error_messages[0].msg,\r\n            });\r\n          } else {\r\n            Swal.fire({\r\n              icon: \"error\",\r\n              title: \"Terjadi Kesalahan!\",\r\n              text: e,\r\n            });\r\n          }\r\n        });\r\n    }\r\n  };\r\n\r\n  let handleChange = (e, field) => {\r\n    let data = e.target.value;\r\n    if (field === \"kandidat\") {\r\n      setNamaKandidat(data);\r\n    } else if (field === \"foto\") {\r\n      setFotoKandidat(data);\r\n    } else {\r\n      Swal.fire({\r\n        icon: \"error\",\r\n        title: \"Oops...\",\r\n        text: \"Something went wrong!\",\r\n      });\r\n    }\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <Card className=\"mt-2 mb-2\" color=\"secondary\">\r\n        <CardBody>\r\n          <Button color=\"primary\" onClick={() => toggleModalCalon(true)}>\r\n            Tambah\r\n          </Button>\r\n        </CardBody>\r\n      </Card>\r\n\r\n      {/* table data calon */}\r\n      <DataTable columns={columnCalon} data={dataCandidate} pagination />\r\n\r\n      {/* modal edit calon */}\r\n      <Modal isOpen={modalCalon} toggle={toggleModalCalon}>\r\n        <ModalHeader toggle={toggleModalCalon}>Modal title</ModalHeader>\r\n        <ModalBody>\r\n          <Form>\r\n            <FormGroup>\r\n              <Label for=\"namaKandidat\">Event</Label>\r\n              <Input\r\n                id=\"namaKandidat\"\r\n                name=\"kandidat\"\r\n                placeholder=\"Nama Calon\"\r\n                type=\"text\"\r\n                value={namaKandidat}\r\n                onChange={(e) => handleChange(e, \"kandidat\")}\r\n              />\r\n            </FormGroup>\r\n            <FormGroup>\r\n              <Label for=\"fotoKandidat\">Foto Calon</Label>\r\n 